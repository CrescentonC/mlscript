// NOTE: rev map on primitive, paper style pushing in
fun map(xs) = if xs is
  C(h, t) then f => C(f(h), map(t, f))
  N then f => N
fun rev(ys) = if ys is
  C(h, t) then a => rev(t, C(h, a))
  N then a => a
fun ff(x) = if x is
  A then AA
  B then BB
rev(map(primitive, ff), N)
