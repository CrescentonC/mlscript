}
downsweep :: (a -> b -> c -> (c,c)) -> c -> BinTree d (a,b) -> BinTree c Unit
downsweep g d (Cell x)      = Cell d
downsweep g d (Node (lv,rv) l r) = case (g lv rv d) of {
  (dl, dr) -> case (downsweep g dl l, downsweep g dr r) of {
