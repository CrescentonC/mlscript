scanL :: (a -> a -> a) -> a -> [a] -> (a,[a])
scanL f u xs =
  let down = \l r x -> (x, f x l) in
  case (sweep_ud f down u (put xs)) of {
    (up_ans, t') -> (up_ans, get t')
  }
scanR :: (a -> a -> a) -> a -> [a] -> (a,[a])
scanR f u xs =
  let down = \l r x -> (f r x, x) in
