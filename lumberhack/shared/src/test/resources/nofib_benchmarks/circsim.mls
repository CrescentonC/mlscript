upsweep :: (a -> a -> a) -> BinTree a b -> (a, BinTree a (a,a))
upsweep f (Cell a)     = (a, Cell a)
upsweep f (Node x l r) = case (upsweep f l) of {
  (lv, l') -> case (upsweep f r) of {
    (rv, r') -> (f lv rv, Node (lv, rv) l' r')
