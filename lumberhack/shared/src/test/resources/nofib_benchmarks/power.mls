  Pz -> Pz
  (Pc f fs) -> (Pc (c*f) (dotMult c fs)))
dotMultSndLz c ps = lazy (case (force (force ps)) of
  Pz -> Pz
  (Pc f fs) -> (Pc (c*f) (dotMult c fs)))
negatePs ps = lazy (case (force ps) of
  Pz -> Pz
  (Pc f fs) -> (Pc (0-f) (negatePs fs)))
addPs fss gs = lazy (case (force fss) of
  Pz -> force gs
