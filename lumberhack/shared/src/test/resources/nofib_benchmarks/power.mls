list   = lazy (Pc 1 list)
ts = lazy (Pc 1 (multPsLzLz (lazy ts) (lazy ts)))
cosx dummy = minusPs (lazy (Pc 1 (lazy Pz))) (integral (integralLz (lazy (cosx 0))))
