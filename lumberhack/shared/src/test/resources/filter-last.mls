:NewParser
:ParseOnly

fun filter(ls, f) = if ls is
  C(h, t) then if f(h) then C(h, filter(t, f)) else filter(t, f)
  N then N
fun lastDrive(ls) = if ls is
  C(h, t) then Some(last(t, h))
  N then None
fun last(a, ls) = if ls is
  N then a
  C(h, t) then last(h, t)
fun lastFilter(ls, f) = lastDrive(filter(ls, f))
lastFilter
